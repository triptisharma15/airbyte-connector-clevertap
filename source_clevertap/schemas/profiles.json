{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": true,
  "properties": {
    "identity": {
      "type": ["null", "string"],
      "description": "User identity/ID"
    },
    "email": {
      "type": ["null", "string"],
      "description": "User email"
    },
    "phone": {
      "type": ["null", "string"],
      "description": "User phone number"
    },
    "name": {
      "type": ["null", "string"],
      "description": "User name"
    }
  }
}

